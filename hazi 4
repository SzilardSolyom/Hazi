#include <iostream>
#include <string>
#include <fstream>

using namespace std;

int main()
{
    ifstream in("input.txt");
    ofstream out("output.txt");
    string s;
    cout << "kerem a szoveget-";
    cin >> s;
    int n = s.size();
    int k;
    cout << k;
    char first=s[n-1];
    while(k<=5)
    for(int i=n;i>0;i--){
        s[i]=s[i-k];
    }
    s[0]= first;
    out<< s << endl;

    return 0;
}

